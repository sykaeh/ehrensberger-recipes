{
  "name": "ehrensberger-recipes",
  "version": "1.0.0",
  "description": "Recipes of the Ehrensberger family",
  "scripts": {
    "build-dev:webpack": "webpack",
    "watch:webpack": "webpack --watch",
    "watch:eleventy": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "dev": "npm-run-all clean build-dev:webpack --parallel watch:*",
    "build:webpack": "cross-env NODE_ENV=production webpack --mode production",
    "build:eleventy": "cross-env ELEVENTY_ENV=production eleventy",
    "build": "run-s clean build:*",
    "serve:local": "serve _site",
    "serve": "run-s build serve:local",
    "clean": "rimraf _site",
    "format:js": "prettier '**/*.js' --write && eslint '**/*.js' --fix",
    "format:json": "prettier '**/*.json' --write",
    "format": "run-s format:*",
    "lint": "eslint --ext .js ."
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:sykaeh/ehrensberger-recipes.git"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "autoprefixer": "~10.4.13",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "~7.0.3",
    "css-loader": "~6.7.2",
    "cssnano": "~5.1.14",
    "eleventy-plugin-lazyimages": "^2.1.2",
    "eslint": "~8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "file-loader": "~6.2.0",
    "glob": "^8.0.3",
    "html-minifier": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.1.0",
    "mini-css-extract-plugin": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "serve": "^14.0.1",
    "tailwindcss": "~3.2.4",
    "webpack": "~5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-remove-empty-scripts": "~1.0.1"
  }
}
